raph","inlineContent":[{"type":"strong","inlineContent":[{"text":"Responses:","type":"text"}]}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"422"},{"text":": Referenced response","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"201"},{"text":": Response when a new invitation is created","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"403"},{"text":": Referenced response","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"204"},{"type":"text","text":": Response when:"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"an existing collaborator is added as a collaborator"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"an organization member is added as an individual collaborator","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"an existing team member (whose team is also a repository collaborator) is added as an individual collaborator"}],"type":"paragraph"}]}]}]}],"hierarchy":{"paths":[["doc:\/\/com.github.api\/documentation\/GitHub-v3-REST-API","doc:\/\/com.github.api\/documentation\/GitHub-v3-REST-API\/_repos__owner___repo__collaborators__username_"]]},"variants":[{"traits":[{"interfaceLanguage":"openapi"}],"paths":["\/documentation\/github-v3-rest-api\/_repos__owner___repo__collaborators__username_\/put"]}],"metadata":{"fragments":[{"kind":"text","text":"PUT"}],"symbolKind":"method","navigatorTitle":[{"kind":"text","text":"PUT \/repos\/{owner}\/{repo}\/collaborators\/{username}"}],"role":"symbol","externalID":"operation:put:\/repos\/{owner}\/{repo}\/collaborators\/{username}","title":"Add a repository collaborator","roleHeading":"HTTP PUT","modules":[{"name":"GitHub v3 REST API"}]},"identifier":{"interfaceLanguage":"openapi","url":"doc:\/\/com.github.api\/documentation\/GitHub-v3-REST-API\/_repos__o