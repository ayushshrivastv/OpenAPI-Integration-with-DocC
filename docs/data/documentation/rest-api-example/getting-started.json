{"kind":"article","identifier":{"url":"doc:\/\/com.example.API\/documentation\/REST-API-Example\/Getting-Started","interfaceLanguage":"swift"},"schemaVersion":{"minor":3,"major":0,"patch":0},"sections":[],"metadata":{"role":"article","roleHeading":"Article","title":"Getting Started"},"abstract":[{"text":"Learn how to integrate with our REST API.","type":"text"}],"hierarchy":{"paths":[["doc:\/\/com.example.API\/documentation\/REST-API-Example"]]},"primaryContentSections":[{"content":[{"anchor":"Overview","level":2,"text":"Overview","type":"heading"},{"inlineContent":[{"text":"This guide will help you get started with integrating our REST API into your application. Our API uses standard HTTP methods and JSON for request and response bodies.","type":"text"}],"type":"paragraph"},{"text":"Authentication","level":2,"type":"heading","anchor":"Authentication"},{"type":"paragraph","inlineContent":[{"text":"All endpoints require authentication. Include your API key in the headers of each request:","type":"text"}]},{"code":["var request = URLRequest(url: url)","request.setValue(\"Bearer YOUR_API_KEY\", forHTTPHeaderField: \"Authorization\")"],"syntax":"swift","type":"codeListing"},{"type":"heading","level":2,"text":"Using the API","anchor":"Using-the-API"},{"type":"heading","level":3,"anchor":"Swift-Example","text":"Swift Example"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Hereâ€™s a Swift example of how to get a list of users:"}]},{"code":["import Foundation","","func fetchUsers(completion: @escaping ([User]?, Error?) -> Void) {","    let url = URL(string: \"https:\/\/api.example.com\/users\")!","    var request = URLRequest(url: url)","    request.httpMethod = \"GET\"","    request.setValue(\"Bearer YOUR_API_KEY\", forHTTPHeaderField: \"Authorization\")","","    let task = URLSession.shared.dataTask(with: request) { data, response, error in","        if let error = error {","            completion(nil, error)","            return","        }","","        guard let data = data else {","            completion(nil, NSError(domain: \"No data\", code: 0, userInfo: nil))","            return","        }","","        do {","            let users = try JSONDecoder().decode([User].self, from: data)","            completion(users, nil)","        } catch {","            completion(nil, error)","        }","    }","","    task.resume()","}"],"syntax":"swift","type":"codeListing"},{"level":3,"type":"heading","anchor":"Swift-AsyncAwait-Example","text":"Swift Async\/Await Example"},{"type":"paragraph","inlineContent":[{"text":"Using modern Swift concurrency:","type":"text"}]},{"type":"codeListing","code":["import Foundation","","func fetchUsers() async throws -> [User] {","    let url = URL(string: \"https:\/\/api.example.com\/users\")!","    var request = URLRequest(url: url)","    request.httpMethod = \"GET\"","    request.setValue(\"Bearer YOUR_API_KEY\", forHTTPHeaderField: \"Authorization\")","","    let (data, _) = try await URLSession.shared.data(for: request)","    return try JSONDecoder().decode([User].self, from: data)","}"],"syntax":"swift"},{"level":2,"text":"Error Handling","type":"heading","anchor":"Error-Handling"},{"type":"paragraph","inlineContent":[{"text":"Our API uses standard HTTP status codes:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"200 - OK: Request succeeded","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"400 - Bad Request: Invalid parameters"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"401 - Unauthorized: Authentication failed"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"404 - Not Found: Resource not found","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"500 - Server Error: Something went wrong on our side"}],"type":"paragraph"}]}]},{"anchor":"Rate-Limits","text":"Rate Limits","level":2,"type":"heading"},{"type":"paragraph","inlineContent":[{"text":"API requests are limited to 100 requests per minute per API key.","type":"text"}]}],"kind":"content"}],"references":{"doc://com.example.API/documentation/REST-API-Example":{"identifier":"doc:\/\/com.example.API\/documentation\/REST-API-Example","url":"\/documentation\/rest-api-example","role":"collection","title":"REST API Example","abstract":[],"type":"topic","kind":"article"}}}